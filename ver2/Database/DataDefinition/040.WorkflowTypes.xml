				<WorkflowDefinitionVersionRole t:Id="role-prevassignee">
					<WorkflowRole ID="{$wfrole-prevassignee.ID}" Name="{$wfrole-prevassignee.Name}" RoleType="{$wfrole-prevassignee.RoleType}" />
