		<FormTemplate t:Id="form-role-permissions" Name="Template for workflow permissions" />
		<FormTemplateVersion t:Id="form-role-permissions-ver1" Version="1" TemplateID="{$form-role-permissions.ID}" />

		<WorkflowRole t:Id="wfrole1" Name="XLS_kio_role1" />

		<WorkflowDefinition t:Id="wftype1" Name="UDF_Workflow Type" IdGenScheme="{Year}.{Month}.{Day}.{Hour}.{AlphaID}"/>
		<WorkflowDefinitionVersionWithAll t:Id="wftype1-v1" Configuration=""
			WorkflowDefinitionID="{$wftype1.ID}" Comments="" Version="1">

			<StepActions>
				<WorkflowDefinitionVersionStepAction t:Id="go-to-next-step" Name="go to next step" DestinationStepID="{$this:step2.ID}" Flags="0" />
			</StepActions>

			<Steps>
				<WorkflowDefinitionVersionStepWithMaps t:Id="step1" Name="S1" StatusName="start" Order="0">
					<FormTemplateVersion ID="{$ftv-ft1-v1.ID}" Name="{$ft1.Name}" />
					<WorkflowDefinitionVersionRole ID="{$this:role-creator.ID}" />
					<StepActions>
						<WorkflowDefinitionVersionStepMapItem MappedID="{$this:go-to-next-step.ID}">
							<Roles>
								<MappedIdUnique ID="{$this:role-creator.ID}" />
								<MappedIdUnique ID="{$this:role-assignee.ID}" />
							</Roles>
						</WorkflowDefinitionVersionStepMapItem>
					</StepActions>
					<StepPermissions>
						<WorkflowDefinitionRoleStepPermission RoleID="{$this:role-creator.ID}" LocalPermission="7" GlobalPermission="7" />
					</StepPermissions>
				</WorkflowDefinitionVersionStepWithMaps>

				<WorkflowDefinitionVersionStepWithMaps t:Id="step2" Name="S2" StatusName="process" Order="1">
					<FormTemplateVersion ID="{$ftv-ft1-v1.ID}" Name="{$ft1.Name}" />
					<WorkflowDefinitionVersionRole ID="{$this:role-assignee.ID}" />
					<StepPermissions>
						<WorkflowDefinitionRoleStepPermission RoleID="{$this:role-assignee.ID}" LocalPermission="7" GlobalPermission="0" />
					</StepPermissions>
				</WorkflowDefinitionVersionStepWithMaps>
			</Steps>
		</WorkflowDefinitionVersionWithAll>

